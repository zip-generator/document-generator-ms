  "dependencies": {
    "@nestjs/common": "^10.0.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/microservices": "^10.4.1",
    "@nestjs/platform-express": "^10.0.0",
    "@prisma/client": "^5.19.1",
    "@types/archiver": "^6.0.2",
    "archiver": "^7.0.1",
    "carbone": "^3.5.6",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "currency.js": "^2.0.4",
    "date-fns": "^3.6.0",
    "date-fns-tz": "^3.1.3",
    "dotenv": "^16.4.5",
    "joi": "^17.13.3",
    "lodash": "^4.17.21",
    "nats": "^2.28.2",
    "pdf-lib": "^1.17.1",
